# Telegram Bot Environment Variables
BOT_TOKEN=your_bot_token_here
ADMIN_USER_ID=your_admin_user_id_here
WEB_SECRET_KEY=your-super-secret-key-for-web-app-2024

# Optional API Keys
METALPRICEAPI_KEY=demo
API_NINJAS_KEY=demo
FMP_API_KEY=demo
ALPHA_VANTAGE_KEY=demo
EIA_API_KEY=demo

# Web App URL (for integration with bot)
WEB_APP_URL=https://your-app-name.railway.app

# OpenAI API Key (for ChatGPT and AI digests)
OPENAI_API_KEY=your_openai_api_key_here

# AI Digest Settings (optional)
# Enable AI digests feature
DIGEST_ENABLED=false

# Telegram API for monitoring channels (get from https://my.telegram.org/)
TELEGRAM_API_ID=your_telegram_api_id
TELEGRAM_API_HASH=your_telegram_api_hash

# Qdrant Vector Database (for duplicate detection)
# You can use free tier at https://cloud.qdrant.io/
QDRANT_URL=https://your-qdrant-instance.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key

# Channels to monitor (comma-separated, e.g., @channel1,@channel2)
DIGEST_SOURCE_CHANNELS=@channel1,@channel2

# Destination channel for publishing digests
DIGEST_DEST_CHANNEL=@your_digest_channel

# Digest settings
DIGEST_COLLECTION_NAME=telegram_digest
DIGEST_SIMILARITY_THRESHOLD=0.85
DIGEST_EMBEDDING_MODEL=text-embedding-3-small
DIGEST_LLM_MODEL=gpt-4o-mini
DIGEST_PUBLISH_SCHEDULE=daily 